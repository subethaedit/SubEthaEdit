<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE syntax SYSTEM "symbol.dtd">
<symbols>

    <symbol id="Separators">
        <regex>^-{5,}()</regex>
    </symbol>

    <symbol id="Conference" font-weight="bold" font-style="normal" indentation="0" ignoreblocks="yes" image="SymbolC">
        <regex>(?&lt;=[\n\r]|^)CONFERENCE:\s*([^\n\r]+)</regex>
    </symbol>
    <symbol id="Date" font-weight="normal" font-style="italic" indentation="0" ignoreblocks="yes">
        <regex>(?&lt;=[\n\r]|^)DATE:\s*([^\n\r]+)</regex>
    </symbol>

    <symbol id="SECTIONS" font-weight="bold" font-style="normal" indentation="0" ignoreblocks="yes">
        <regex>-{5}[\n\r]{1,2}([^:\n\r]+):</regex>
    </symbol>

    <symbol id="HEADINGS" font-weight="normal" font-style="normal" indentation="0" ignoreblocks="yes" image="SymbolH">
        <regex>^([[:upper:]0-9]+[[:upper:]0-9 &amp;_\-/'!?.]*)[\n\r]</regex>
    </symbol>

    <symbol id="Events" font-weight="normal" font-style="italic" indentation="1" ignoreblocks="yes">
        <regex>(?&lt;=[\n\r]|^)\*\*\* ([^\n\r\*]+) \*\*\*[\n\r]</regex>
    </symbol>

</symbols>