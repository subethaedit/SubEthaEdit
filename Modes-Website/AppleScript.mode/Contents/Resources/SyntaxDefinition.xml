            </keywords>
            
            <keywords id="Numbers" useforautocomplete="no" scope="language.constant.numeric">
                <regex>([-&amp;+\*\\\(\)])</regex>
				<regex>[^[A-Za-z]]([0-9]*\.*[0-9]+)[^[A-Za-z]]</regex>
				<regex>(^[0-9]*\.*[0-9]+)[^[A-Za-z]]</regex>
				<regex>[^[A-Za-z]]([0-9]*\.*[0-9]+)</regex>
				<regex>(0x[0-9a-fA-F]+)</regex>
            </keywords>
            
            <keywords id="Characters" useforautocomplete="no" scope="language.constant.numeric.character">
				<regex>'.'</regex>
            </keywords>
            
            <state id="Comment" type="comment" scope="comment.block">
                <begin><string>\(\*</string></begin>
                <end><regex>\*\)</regex></end>
            </state>
            
            <state id="SingleComment" type="comment" scope="comment.line">
                <begin><string>--</string></begin>
                <end><regex>[\n\r]</regex></end>
            </state>
            
            <state id="String" type="string" scope="string.double">
                <begin><string>@?"</string></begin>
                <end><regex>((?&lt;!\\)(\\\\)*)"</regex></end>
            </state>

            <state id="Tell statement with to" foldable="no" scope="meta.block">
                <begin><regex>tell[\t ]+(?=[^\n\r]+to)</regex></begin>
                <end><regex>to</regex></end>
                <import mode="AppleScript"/>
            </state>

            <state id="Tell statement" foldable="yes" scope="meta.block">
                <begin><regex>tell[\t ]+[^\n\r]+</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+tell)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="Repeat Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*repeat(?:[ \t]+[^\n\r]+)?(?=[\n\r])</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+repeat)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="Try Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*try(?=\s)</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+try)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="using terms from Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*using terms from[ \t]+[^\n\r]*</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+using terms from)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="with timeout Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*with timeout[ \t]+[^\n\r]*</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+with timeout)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="with transaction Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*with transaction(?:[ \t]+[^\n\r]+)?(?=[\n\r])</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+with transaction)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="ignoring Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*ignoring[ \t]+[^\n\r]*</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+ignoring)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
            <state id="considering Statement" foldable="yes" scope="meta.block">
                <begin><regex>^[ \t]*considering[ \t]+[^\n\r]*</regex></begin>
                <end><regex>^\s*(?&lt;trimmedend&gt;end(?:[ \t]+considering)?)(?=([\n\r]|\Z))</regex></end>
                <import mode="AppleScript"/>
            </state>
        
         </default>
    </states>
</syntax>
